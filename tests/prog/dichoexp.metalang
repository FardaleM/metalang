
integer exp(integer a, integer b){
	if (b == 0){
	   return 1;
	}
	if (b % 2 == 0){
	   integer o := exp(a, b / 2);
	   return o * o;
	}else{
	   return a * exp(a, b - 1);
	}
}

prog{
	integer a := 0;
	integer b := 0;
	read<integer>(a);
	stdin_sep();
	read<integer>(b);
	print<integer>(exp(a, b));
}