function integer double_(integer %a){
	 if (%a == 0){
	    return %a * 2;
	 }else{
		return %a * 2;
	 }
}

function void foo(integer %a, float %b){
	 %a := double_(%a);
	 print<float>(%a + double_(%b));
}

prog{
	var integer %a := 2;
	var integer %b := 3;
	var integer %i := 0;
	read<integer>(%a);
	dim integer array %tab[%a];
	for %i := 0 to %a step 1
	{
		%tab[%i] := %i * (%i + 1) / 2;
		%b := %b + %tab[%i];
	}
	foo(%a, %b);
	print<integer>(%b + %a);
	print<integer array>(%tab);
}