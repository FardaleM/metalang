integer nth(char array tab, char tofind, integer len){
	integer out := 0;
	for i := 0 to len - 1
	{
		if (tab[i] == tofind){
		   out := out +1;
		}
	}
	return out;
}


prog{
	integer len := 0;
	read<integer>(len);
	stdin_sep();
	char tofind := '\0';
	read<char>(tofind);
	stdin_sep();
	char array tab[len + 1];
	for i := 0 to len - 1
	{
		char tmp := '\0';
		read<char>(tmp);
		tab[i] := tmp;
	}
	tab[len] := '\0';
	integer result := nth(tab, tofind, len);
	print<integer>(result);
}