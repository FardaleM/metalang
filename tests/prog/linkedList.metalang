record intlist
  head : int
  tail : intlist
end

def intlist cons(intlist list, int i)
  def intlist out = record
    head = i
    tail = list
  end
  return out
end

def intlist rev2(intlist empty, intlist acc, intlist torev)
  if torev == empty then
    return acc
  else
    def intlist acc2 = record
      head = torev.head
      tail = acc
    end
    return rev2(empty, acc, torev.tail);
  end
end

def intlist rev(intlist empty, intlist torev)
  return rev2(empty, empty, torev);
end

def void test(intlist empty)
  def intlist list = empty
  def auto i = -1
  while i != 0 do
    read auto i
    if i != 0 then
      list = cons(list, i)
    end
