
def int score()
	skip def read int len skip
	def sum = 0
	for i = 1 to len do
		def read char c
		sum += int_of_char(c) - int_of_char('A') + 1
/*		print c print " " print sum print " " */
  end
	return sum
end

main
	def sum = 0
	def read int n
	for i = 1 to n do
		sum += i * score()
	end
	print sum print "\n"
end
