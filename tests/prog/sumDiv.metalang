integer sumdiv(integer n){
	/* On désire renvoyer la somme des diviseurs */
	integer out := 0; /* On déclare un entier qui contiendra la somme */
	for i := 1 to n /* La boucle : i est le diviseur potentiel*/
	   if (n % i == 0) /* Si i divise */
	      out := out + i; /* On incrémente */
	   else{ /* nop */ }
	return out; /*On renvoie out*/
}

prog{ /* Programme principal */
integer n := 0;
read<integer>(n); /* Lecture de l'entier */
print<integer>(sumdiv(n));
