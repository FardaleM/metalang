integer go(integer array tab, integer a, integer b) {
	integer m := (a + b) / 2;
	if (a == m) {
		if (tab[a] == m) { return b; }
		else { return a; }
	}
	integer i := a;
	integer j := b;
	while (i < j) {
		integer e := tab[i];
		if (e < m) {
			i := i + 1;
		} else {
			j := j - 1;
			tab[i] := tab[j];
			tab[j] := e;
		}
	}

	if (i < m) {
		return go(tab, a, m);
	} else {
		return go(tab, m, b);
	}
}

integer plus_petit(integer array tab, integer len) {
	return go(tab, 0, len);
}

prog {
	integer len := 0;
	read<integer>(len);
	stdin_sep();
	integer array tab[len] (i ->
		integer tmp := 0;
		read<integer>(tmp);
		stdin_sep();
		return tmp;
	);
	print<integer>(plus_petit(tab, len));
}
