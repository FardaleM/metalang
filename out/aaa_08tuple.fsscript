open System
type toto = {
  mutable foo : (int * int);
  mutable bar : int;
};;

let () =
  let bar_ = Console.ReadLine () |> Int32.Parse
  let c = (Console.ReadLine ()).Split [|' '|] |> Array.map Int32.Parse
  let t = {foo=(c.[0], c.[1]);
  bar=bar_}
  let (a, b) = t.foo
  Printf.printf "%d %d %d\n" a b t.bar
 