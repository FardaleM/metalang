)
(define (fillPrimesFactors t0 n primes nprimes)
  ;toto
  (let ([s 0])
  (let ([u (- nprimes 1)])
  (letrec ([q (lambda (i n) 
                (if (<= i u)
                (let ([d (vector-ref primes i)])
                (letrec ([r (lambda (n) 
                              (if (eq? (remainder n d) 0)
                              (block
                                (vector-set! t0 d (+ (vector-ref t0 d) 1))
                                (let ([n (quotient n d)])
                                (r n))
                                )
                              (if (eq? n 1)
                              (vector-ref primes i)
                              (q (+ i 1) n))))])
                (r n)))
                n))])
  (q s n))))
)
