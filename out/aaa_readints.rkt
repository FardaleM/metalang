#lang racket
(require racket/block)
(define array_init_withenv (lambda (len f env)
  (build-vector len (lambda (i)
    (let ([o ((f i) env)])
      (block
        (set! env (car o))
        (cadr o)
      )
    )))))

(define main
  (let ([len (string->number (read-line))])
  (block
    (map display (list len "=len\n"))
    (let ([tab1 (list->vector (map string->number (regexp-split " " (read-line))))])
    (let ([v 0])
    (let ([w (- len 1)])
    (letrec ([u (lambda (i) 
                  (if (<= i w)
                  (block
                    (map display (list i "=>" (vector-ref tab1 i) "\n"))
                    (u (+ i 1))
                    )
                  (let ([len (string->number (read-line))])
                  (let ([tab2 (array_init_withenv (- len 1) (lambda (h) 
                                                              (lambda (_) 
                                                              (let ([l (list->vector (map string->number (regexp-split " " (read-line))))])
                                                              (list '() l)))) '())])
                  (let ([r 0])
                  (let ([s (- len 2)])
                  (letrec ([m (lambda (i) 
                                (if (<= i s)
                                (let ([p 0])
                                (let ([q (- len 1)])
                                (letrec ([o (lambda (j) 
                                              (if (<= j q)
                                              (block
                                                (map display (list (vector-ref (vector-ref tab2 i) j) " "))
                                                (o (+ j 1))
                                                )
                                              (block
                                                (display "\n")
                                                (m (+ i 1))
                                                )))])
                                (o p))))
                                '()))])
                  (m r))))))))])
    (u v)))))
))
)

