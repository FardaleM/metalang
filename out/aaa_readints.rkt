#lang racket
(require racket/block)
(define array_init_withenv (lambda (len f env)
  (build-vector len (lambda (i)
    (let ([o ((f i) env)])
      (block
        (set! env (car o))
        (cadr o)
      )
    )))))

(define (read_int_matrix x y)
  ;toto
  (let ([tab (array_init_withenv y (lambda (z) 
                                     (lambda (_) (let ([a (list->vector (map string->number (regexp-split " " (read-line))))])
                                                 (let ([e a])
                                                 (list '() e))))) '())])
  tab)
)
(define main
  (let ([len (string->number (read-line))])
  (block
    (map display (list len "=len\n"))
    (let ([tab1 (list->vector (map string->number (regexp-split " " (read-line))))])
    (let ([p 0])
    (let ([q (- len 1)])
    (letrec ([o (lambda (i) 
                  (if (<= i q)
                  (block
                    (map display (list i "=>" (vector-ref tab1 i) "\n"))
                    (o (+ i 1))
                    )
                  (let ([len (string->number (read-line))])
                  (let ([tab2 (read_int_matrix len (- len 1))])
                  (let ([l 0])
                  (let ([m (- len 2)])
                  (letrec ([f (lambda (i) 
                                (if (<= i m)
                                (let ([h 0])
                                (let ([k (- len 1)])
                                (letrec ([g (lambda (j) 
                                              (if (<= j k)
                                              (block
                                                (map display (list (vector-ref (vector-ref tab2 i) j) " "))
                                                (g (+ j 1))
                                                )
                                              (block
                                                (display "\n")
                                                (f (+ i 1))
                                                )))])
                                (g h))))
                                '()))])
                  (f l))))))))])
    (o p)))))
))
)

