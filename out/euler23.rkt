#lang racket
(require racket/block)
(define array_init_withenv (lambda (len f env)
  (build-vector len (lambda (i)
    (let ([o ((f i) env)])
      (block
        (set! env (car o))
        (cadr o)
      )
    )))))

(define (eratostene t0 max0)
  ;toto
  (let ([n 0])
  (let ([bc 2])
  (let ([bd (- max0 1)])
  (letrec ([ba (lambda (i n) 
                 (if (<= i bd)
                 (let ([n (if (eq? (vector-ref t0 i) i)
                          (let ([n (+ n 1)])
                          (let ([j (* i i)])
                          (letrec ([bb (lambda (j) 
                                         (if (and (< j max0) (> j 0))
                                         (block
                                           (vector-set! t0 j 0)
                                           (let ([j (+ j i)])
                                           (bb j))
                                           )
                                         n))])
                          (bb j))))
                 n)])
                 (ba (+ i 1) n))
                 n))])
  (ba bc n)))))
)
(define (fillPrimesFactors t0 n primes nprimes)
  ;toto
  (let ([y 0])
  (let ([z (- nprimes 1)])
  (letrec ([w (lambda (i n) 
                (if (<= i z)
                (let ([d (vector-ref primes i)])
                (letrec ([x (lambda (n) 
                              (if (eq? (remainder n d) 0)
                              (block
                                (vector-set! t0 d (+ (vector-ref t0 d) 1))
                                (let ([n (quotient n d)])
                                (x n))
                                )
                              (if (eq? n 1)
                              (vector-ref primes i)
                              (w (+ i 1) n))))])
                (x n)))
                n))])
  (w y n))))
)
