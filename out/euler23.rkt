(define (eratostene t0 max0)
  ;toto
  (let ([n 0])
  (let ([w (- max0 1)])
  (letrec ([u (lambda (i n) 
                (if (<= i w)
                (if (eq? (vector-ref t0 i) i)
                (let ([n (+ n 1)])
                (let ([j (* i i)])
                (letrec ([v (lambda (j) 
                              (if (and (< j max0) (> j 0))
                              (block
                                (vector-set! t0 j 0)
                                (let ([j (+ j i)])
                                (v j))
                                )
                              (u (+ i 1) n)))])
                (v j))))
                (u (+ i 1) n))
                n))])
  (u 2 n))))
)
(define (fillPrimesFactors t0 n primes nprimes)
  ;toto
  (let ([m (- nprimes 1)])
  (letrec ([g (lambda (i n) 
                (if (<= i m)
                (let ([d (vector-ref primes i)])
                (letrec ([h (lambda (n) 
                              (if (eq? (remainder n d) 0)
                              (block
                                (vector-set! t0 d (+ (vector-ref t0 d) 1))
                                (let ([n (quotient n d)])
                                (h n))
                                )
                              (if (eq? n 1)
                              (vector-ref primes i)
                              (g (+ i 1) n))))])
                (h n)))
                n))])
  (g 0 n)))
)
