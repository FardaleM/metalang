(define (sumdiv nprimes primes n)
  ;toto
  (let ([t0 (array_init_withenv (+ n 1) (lambda (i) 
                                          (lambda (_) (let ([e 0])
                                                      (list '() e)))) '())])
  (let ([max0 (fillPrimesFactors t0 n primes nprimes)])
  (sumdivaux t0 max0 0)))
