(define array_init_withenv (lambda (len f env)
  (build-vector len (lambda (i)
    (let ([o ((f i) env)])
      (block
        (set! env (car o))
        (cadr o)
      )
    )))))

(define (eratostene t0 max0)
  ;toto
  (let ([n 0])
  (let ([z 2])
  (let ([ba (- max0 1)])
  (letrec ([x (lambda (i n) 
                (if (<= i ba)
                (let ([n (if (eq? (vector-ref t0 i) i)
                         (let ([n (+ n 1)])
                         (let ([j (* i i)])
                         (letrec ([y (lambda (j) 
                                       (if (and (< j max0) (> j 0))
                                       (block
                                         (vector-set! t0 j 0)
                                         (let ([j (+ j i)])
                                         (y j))
                                         )
                                       n))])
                         (y j))))
                n)])
                (x (+ i 1) n))
                n))])
  (x z n)))))
)
(define (fillPrimesFactors t0 n primes nprimes)
  ;toto
  (let ([v 0])
  (let ([w (- nprimes 1)])
  (letrec ([m (lambda (i n) 
                (if (<= i w)
                (let ([d (vector-ref primes i)])
                (letrec ([u (lambda (n) 
                              (if (eq? (remainder n d) 0)
                              (block
                                (vector-set! t0 d (+ (vector-ref t0 d) 1))
                                (let ([n (quotient n d)])
                                (u n))
                                )
                              (if (eq? n 1)
                              (vector-ref primes i)
                              (m (+ i 1) n))))])
                (u n)))
                n))])
  (m v n))))
)
