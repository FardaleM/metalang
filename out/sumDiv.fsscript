let sumdiv n =
  (* On désire renvoyer la somme des diviseurs *)
  let out0 = ref( 0 )
  (* On déclare un entier qui contiendra la somme *)
  for i = 1 to n do
    (* La boucle : i est le diviseur potentiel*)
    if n % i = 0 then
        (* Si i divise *)
        out0 := (!out0) + i
        (* On incrémente *)
    else
        (* nop *)
        ()
  (!out0)
  (*On renvoie out*)

