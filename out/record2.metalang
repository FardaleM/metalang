record toto int foo;
int bar;
int blah;

end

def @toto mktoto(int v1)
  def @toto t = with foo = v1
  bar = 0
  blah = 0 end
  return t
end

def int result(@toto t)
  t.blah = t.blah + 1;
  return (t.foo + (t.blah * t.bar)) + (t.bar * t.foo)
end

main
  def @toto t = mktoto(4)
  read int t.bar
  skip
  read int t.blah
  print int result(t)
  print int t.blah
end

