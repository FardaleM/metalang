def int eratostene (array<int> t, int max)
  def int sum = 0
  for i = 2 to  max - 1
  do
    if t[i] == i
    then
      sum = sum + i
      if max / i > i
      then
        def int j = i * i
        while j < max && j > 0
        do
          t[j] = 0
          j = j + i
        end
      end
    end
  end
  return sum
end

