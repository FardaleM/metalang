program sqrttest;
Uses math;


var
  b : Longint;
  e : Longint;
  g : Longint;
  i : Longint;
  k : Longint;
  m : Longint;
begin
  b := 4;
  Write(Floor(Sqrt(b)));
  Write(' ');
  e := 16;
  Write(Floor(Sqrt(e)));
  Write(' ');
  g := 20;
  Write(Floor(Sqrt(g)));
  Write(' ');
  i := 1000;
  Write(Floor(Sqrt(i)));
  Write(' ');
  k := 500;
  Write(Floor(Sqrt(k)));
  Write(' ');
  m := 10;
  Write(Floor(Sqrt(m)));
  Write(' ');
end.


