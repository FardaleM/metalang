Imports System

Module min3

  
  Sub Main()
    Console.Write(Math.Min(Math.Min(2, 3), 4) & " " & Math.Min(Math.Min(2, 4), 3) & " " & Math.Min(Math.Min(3, 2), 4) & " " & Math.Min(Math.Min(3, 4), 2) & " " & Math.Min(Math.Min(4, 2), 3) & " " & Math.Min(Math.Min(4, 3), 2) & Chr(10))
  End Sub
  
End Module

